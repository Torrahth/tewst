Items: {
	AntriquePendent: {
		DisplayName: Antrique Pendent
		Tooltip:
			'''
			'As dusty it may be, its markings still resemble something.'
			Falling off tall ledges will result in a heavy slam
			Gives the user no fall damage
			'''
	}

	Obometer: {
		DisplayName: Obometer
		Tooltip: Increases Attack speed at the cost of your Life
	}

